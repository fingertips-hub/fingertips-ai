{
  "id": "built-in-date",
  "name": "日期",
  "version": "1.0.0",
  "description": "显示当前日期，支持自定义格式",
  "author": "Fingertips AI",
  "type": "simple",
  "category": "collapsed",
  "config": {
    "format": "YYYY-MM-DD",
    "updateInterval": 60000
  },
  "template": {
    "type": "text",
    "content": "{{date}}"
  },
  "styles": {
    "fontSize": "14px",
    "fontWeight": "500",
    "color": "rgba(0, 0, 0, 0.8)"
  }
}
